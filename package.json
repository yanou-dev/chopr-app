{
  "name": "chopr",
  "version": "1.0.0",
  "description": "A modern desktop application for visualizing logs from various sources",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run start\" \"npm run dev:react\"",
    "dev:react": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:build": "npm run build && electron-builder",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "patch-package"
  },
  "build": {
    "appId": "dev.yanou.Chopr",
    "productName": "Chopr",
    "files": [
      "!node_modules"
    ],
    "directories": {
      "output": "dist"
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/x-data-grid": "^8.1.0",
    "@mui/x-data-grid-pro": "^8.1.0",
    "electron-is-dev": "^2.0.0",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "electron": "^25.3.0",
    "electron-builder": "^24.4.0",
    "jest": "^27.5.1",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react-app-rewired": "^2.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}